{
  "dependencies": {
    "core-js": "^3.2.1",
    "date-fns": "^2.4.1",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.0",
    "html-webpack-plugin": "^3.2.0",
    "htmlhint": "^0.11.0",
    "husky": "^3.0.7",
    "json5-loader": "^3.0.0",
    "lint-staged": "^9.4.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "moment-locales-webpack-plugin": "^1.1.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "scripts": {
    "serve:dev": "webpack-dev-server --mode development",
    "serve:prod": "live-server dist",
    "build:dev": "rimraf dist && webpack --mode development",
    "build:prod": "rimraf dist && webpack --mode production",
    "lint": "npm-run-all lint:*",
    "lint:html": "htmlhint src/**/*.html",
    "lint:js": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.{js,ts,json,scss,html}"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.html": [
      "prettier --write",
      "htmlhint",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 3 chrome versions",
    "last 2 safari versions",
    "ie 11"
  ]
}
